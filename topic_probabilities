from gensim import corpora, models
from nltk.corpus import stopwords
from nltk.tokenize import word_tokenize
from multiprocessing import Pool, freeze_support  # Ensure you import Pool and freeze_support
import random
from nltk import pos_tag
from nltk.tokenize import word_tokenize



def process_document(document):
    # Tokenize and remove stop words
    random.seed(8)
    stop_words = set(stopwords.words('english'))
    with open("C:/Users/Maria Konovalov/Downloads/English_Names_All.csv", "r") as f:
        name_stopwords = f.read()
    name_stopwords_list = name_stopwords.split()
    stop_words.update(name_stopwords_list)
    
    tokens = [word for word in word_tokenize(document.lower()) if (word not in stop_words and word.isalpha())]
    # Perform POS tagging
    
    return tokens

